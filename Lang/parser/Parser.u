func isAlphabet(char c){
	return (c == 'a'|'b'|'c'|'d'|'e'|'f'|'g'|'h'|'i'|'j'|'k'|'l'|'m'|'n'|'o'|'p'|'q'|'r'|'s'|'t'|'u'|'v'|'w'|'x'|'y'|'z')
}


func len(dataType){
	$len = 0;

	for($i in dataType){
		len += 1
	}
}

func sizeOf(item){
	return when(item){
		int => 4,
		double => 8,
		float => 8,
		long long => 12,
		char => 4,
		default => 0
	}
}